<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="RaisedBtnTheme" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">@color/colorRaisedBtnNormal</item>
        <item name="colorControlHighlight">@color/colorRaisedBtnHighlight</item>
        <item name="android:textColor">@color/colorRaisedBtnTextColor</item>
    </style>

</resources>
